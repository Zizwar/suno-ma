{
  "expo": {
    "scheme": [
      "https",
      "suno"
    ],
    "platforms": [
      "android"
    ],
    "name": "suno-ma",
    "slug": "suno-music-ai",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wino.suno.ma",
      "permissions": [
        "FOREGROUND_SERVICE",
        "WAKE_LOCK",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.suno.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "*.suno.ai",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "*.suno.ma",
              "pathPrefix": "/"
            },
            {
              "scheme": "suno",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow Suno MA to access your microphone."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dda480e6-1d90-4f7f-a6dc-d2042a5dbf17"
      }
    }
  }
}
